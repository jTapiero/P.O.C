{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./src",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@core/*":["app/core/*"],
      "@page/*":["app/page/*"],
      "@class/*":["app/class/*"],
      "@component/*":["app/component/*"],
      "@deepstream/*":["app/deepstream/*"],
      "@cesium/*":["app/cesium/*"],
      "@service/*":["app/core/service/*"],
      "@pipe/*":["app/core/pipe/*"],
      "@picture/*":["assets/picture/*"],
      "@log/*":["assets/sample-log/*"],
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
